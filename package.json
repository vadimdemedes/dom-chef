{
  "name": "dom-chef",
  "version": "2.0.0",
  "description": "Build DOM elements using JSX automatically",
  "license": "MIT",
  "repository": "vadimdemedes/dom-chef",
  "author": {
    "name": "Vadim Demedes",
    "email": "vdemedes@gmail.com",
    "url": "github.com/vadimdemedes"
  },
  "engines": {
    "node": ">= 4"
  },
  "scripts": {
    "test": "xo && ava"
  },
  "files": [
    "index.js"
  ],
  "keywords": [
    "jsx",
    "dom"
  ],
  "dependencies": {
    "arr-flatten": "^1.0.3",
    "classnames": "^2.2.5",
    "object.omit": "^2.0.1",
    "svg-tag-names": "^1.1.1"
  },
  "devDependencies": {
    "@ava/babel-preset-stage-4": "^1.1.0",
    "ava": "^0.20.0",
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-register": "^6.24.1",
    "browser-env": "^2.0.31",
    "eslint-config-xo-react": "^0.12.0",
    "eslint-plugin-react": "^7.1.0",
    "sinon": "^2.3.6",
    "xo": "^0.18.2"
  },
  "ava": {
    "require": [
      "babel-register"
    ],
    "babel": {
      "presets": [
        "@ava/stage-4"
      ],
      "plugins": [
        [
          "transform-react-jsx",
          {
            "pragma": "h"
          }
        ]
      ]
    }
  },
  "xo": {
    "env": "browser",
    "extends": [
      "xo-react"
    ],
    "plugins": [
      "react"
    ],
    "rules": {
      "react/prop-types": 0,
      "react/no-unknown-property": 0,
      "react/no-danger": 0
    },
    "settings": {
      "react": {
        "pragma": "h"
      }
    }
  }
}
